---
import Container from "../components/Container.astro";

const { element = 'section', settings = {} } = Astro.props;
const Element = element

const sectionId = {
    ...(settings.sectionId && { "id": `${settings.sectionId}` })
}
---

<Element {...sectionId} class={`odd:bg-content-section-odd even:bg-content-section-even py-4`}><Container><slot /></Container></Element>